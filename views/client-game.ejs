 <!DOCTYPE html>
 <html>

 <head>
   <link rel="stylesheet" type="text/css" href="semantic/site/header.css">
   <link rel="stylesheet" type="text/css" href="semantic/site/content.css">
   <link rel="stylesheet" type="text/css" href="semantic/site/font.css">
   <link rel="stylesheet" type="text/css" href="semantic/site/anim.css">
   <link rel="stylesheet" type="text/css" href="semantic/game/game-lobby.css">
   <link rel="stylesheet" type="text/css" href="semantic/game//game-main.css">
   <link href="https://fonts.googleapis.com/css?family=Open+Sans&display=swap" rel="stylesheet">
   <link href="https://fonts.googleapis.com/css?family=Courier+Prime&display=swap" rel="stylesheet">
   <link href="https://fonts.googleapis.com/css?family=Inconsolata&display=swap" rel="stylesheet">
   <title>A Multiplayer Game</title>
   <script src="socket.io/socket.io.js"></script>
   <meta name="viewport" content="width=device-width, height=device-height">
 </head>

 <body>
   <%- include('./elements/header'); -%>

   <div id="volet">
     <div id="logwindow">
       <input type="text" id="userName" placeholder="Nom d'utilisateur ..." /><br>
       <input type="password" id="Password" placeholder="Mot de passe ..." /><br>
       <button id="login"><a>S'inscrire / Se connecter</a></button>
     </div>
   </div>
   <div id="master" style="position: relative;">
     <canvas id="canvas1"></canvas>
     <canvas id="canvas2"></canvas>
     <canvas id="canvas3"></canvas>
   </div>
   <div id="colortable">
     <button class="color" onclick="openFullscreen();">Openfs</button>
     <button class="color" id="c1"></button>
     <button class="color" id="c2"></button>
     <button class="color" id="c3"></button>
   </div>
   <script src="static/game/jquery-3.4.1.min.js"></script>
   <script src="static/game/jquery.touchwipe.1.1.1.js"></script>
   <script src="static/game/client-utils.js"></script>
   <script src="static/game/client-lobby.js"></script>
   <script src="static/game/client-grid.js"></script>
   <script src="static/game/client-movement.js"></script>
   <script src="static/game/client-game.js"></script>

   <%- include('./elements/footer'); -%>
