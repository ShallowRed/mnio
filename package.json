{
  "name": "mnio",
  "version": "0.1.0",
  "description": "Un jeu de tapisserie en ligne",
  "scripts": {
    "prod": "webpack --config webpack.game.config.js",
    "dev": "SET NODE_ENV=dev && webpack --config webpack.game.config.js",
    "devgallery": "SET NODE_ENV=dev && webpack --config webpack.gallery.config.js",
    "prodgallery": "webpack --config webpack.gallery.config.js",
    "test": "node lib/index.js",
    "start": "pm2 start process.json && pm2 logs",
    "reload": "pm2 reload lib/index.js",
    "stop": "pm2 stop lib/index.js",
    "clean": "node data/cleaner.js"
  },
  "browserslist": [
    "last 3 version, > 0.1%, not dead"
  ],
  "dependencies": {
    "core-js": "^3.6.5",
    "d3": "^5.16.0",
    "express": "^4.17.1",
    "hex-to-hsl": "^1.0.2",
    "mysql": "^2.18.1",
    "pm2": "^4.4.0",
    "socket.io": "^2.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "babel-loader": "^8.1.0",
    "css-loader": "^3.5.2",
    "ejs-loader": "^0.3.6",
    "eslint": "^6.8.0",
    "eslint-loader": "^4.0.0",
    "favicons-webpack-plugin": "^3.0.1",
    "html-webpack-plugin": "^4.2.0",
    "mini-css-extract-plugin": "^0.9.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "postcss-normalize": "^9.0.0",
    "postcss-preset-env": "^6.7.0",
    "terser-webpack-plugin": "^2.3.5",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ShallowRed/mnio.git"
  },
  "author": "Lucas Poulain (ShallowRed)",
