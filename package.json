{
	"name": "mnio",
	"version": "0.0.3",
	"description": "Un jeu de tapisserie en ligne",
	"author": "Lucas Poulain (ShallowRed)",
	"license": "ISC",
	"repository": {
	  "type": "git",
	  "url": "git+https://github.com/ShallowRed/mnio.git"
	},
	"homepage": "https://github.com/ShallowRed/mnio#readme",
	"bugs": {
	  "url": "https://github.com/ShallowRed/mnio/issues"
	},
	"engines": {
		"node": ">=12.0.0",
		"npm": ">=6.0.0"
	  },
	"scripts": {
	  "dev": "concurrently \"npm:dev:bundle\" \"npm:dev:server\"",
	  "dev:open": "concurrently \"npm:dev\" \"npm:open\"",
	  "dev:server": "npm run dev --prefix srv",
	  "dev:bundle": "npm run dev --prefix src",
	  "bundle": "npm run prod --prefix src",
	  "open": "open-cli http://localhost:3000",
	  "install:dev": "npm install && npm install --prefix srv && npm install --prefix src",
	  "install:prod": "npm install --only=prod --prefix srv"
	},
	"devDependencies": {
		"concurrently": "^7.1.0",
		"live-server": "^1.2.2",
		"open-cli": "^7.0.1"
	  }
  }
  