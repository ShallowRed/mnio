{
  "name": "mnio",
  "version": "0.1.0",
  "description": "jeu de tapisserie en ligne",
  "dependencies": {
    "core-js": "^3.6.5",
    "express": "^4.17.1",
    "hex-to-hsl": "^1.0.2",
    "mysql": "^2.18.1",
    "pm2": "^4.2.3",
    "regenerator": "^0.14.4",
    "socket.io": "^2.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "autoprefixer": "^9.7.6",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.5.2",
    "ejs-loader": "^0.3.6",
    "eslint": "^6.8.0",
    "eslint-loader": "^4.0.0",
    "favicons-webpack-plugin": "^3.0.1",
    "html-webpack-plugin": "^4.2.0",
    "mini-css-extract-plugin": "^0.9.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "postcss-normalize": "^9.0.0",
    "postcss-preset-env": "^6.7.0",
    "terser-webpack-plugin": "^2.3.5",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "workbox-webpack-plugin": "^5.1.3"
  },
  "browserslist": [
    "last 3 version, > 0.1%, not dead"
  ],
  "postcss": {
    "plugins": {
      "postcss-normalize": {},
      "postcss-preset-env": {},
      "autoprefixer": {}
    }
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "debug": false,
          "useBuiltIns": "usage",
          "corejs": 3
        }
      ]
    ]
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "env": {
      "browser": true,
      "commonjs": true
    },
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module"
    }
  },
  "stylelint": {
    "plugins": [
      "stylelint-no-unsupported-browser-features"
    ],
    "rules": {
      "plugin/no-unsupported-browser-features": true
    },
    "defaultSeverity": "warning"
  },
  "scripts": {
    "prod": "webpack --config config/webpack.config.js",
    "dev": "SET NODE_ENV=dev && webpack --config config/webpack.config.js",
    "start": "pm2 start config/process.json && pm2 logs",
    "test": "node lib/index.js",
    "reload": "pm2 reload lib/index.js",
    "stop": "pm2 stop lib/index.js",
    "prodgallery": "webpack --config config/webpack-gallery.config.js",
    "devgallery": "SET NODE_ENV=dev && webpack --config config/webpack-gallery.config.js",
    "clean": "node data/cleaner.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ShallowRed/mnio.git"
  },
  "author": "Lucas Poulain (ShallowRed)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ShallowRed/mnio/issues"
  },
  "homepage": "https://github.com/ShallowRed/mnio#readme"
}
