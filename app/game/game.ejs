<!DOCTYPE html>
<html>

<head>
	<link href="https://fonts.googleapis.com/css?family=Inconsolata&display=swap" rel="stylesheet">
	<title>MN.io : Un jeu multijoueur du Mobilier National</title>
	<link rel="license" href="/dist/main.js.LICENSE.txt">
	<meta name="viewport" content="width=device-width, height=device-height, initial-scale=1, maximum-scale=1, minimum-scale=1, user-scalable=no">
</head>

<body>
	<a href='/'><img id="logo" src="dist/img/logo-full.png" alt="sorry"></a>
	<%= require('./tutoriel/tutoriel.ejs')() %>

	<section>
		<div id="map" style="position: relative;">
			<canvas></canvas>
			<canvas></canvas>
			<canvas></canvas>

			<div id="player"></div>
			<div id="shadow"></div>
		</div>

		<div id="buttons">
			<button type="button" class="zoom" id="zoomin">+</button>
			<button type="button" class="zoom" id="zoomout">-</button>
			<button type="button" class="color" id="c1"></button>
			<button type="button" class="color" id="c2"></button>
			<button type="button" class="color" id="c3"></button>
			<button type="button" class="color" id="c4"></button>
			<button type="button" class="color" id="c5"></button>
		</div>
	</section>

	<div id="utils">
		<button id="refresh" type="button" onclick="window.location='/'">
			<img src="dist/img/refresh.png" alt="sorry">
		</button>
		<button id="openTuto" type="button">
			<img src="dist/img/what.png" alt="sorry">
		</button>
	</div>

</body>

</html>